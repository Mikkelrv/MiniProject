@page "/listings"
@inject HttpClient Http
<SearchComponent></SearchComponent>
<DisplayAllItemsComponent items="@_items" />


@code {

	private User? _user;
	private List<Item> _items;
	private Filter filter = new(){ Status = "Active"};

	protected async override Task OnInitializedAsync()
	{
		_items = await Http.GetFromJsonAsync<List<Item>>($"api/items/get/all?filter={filter}") ?? new List<Item>(); 
	}

	
}